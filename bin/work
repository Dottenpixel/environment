
#setup SPX-WEB
tmux new-session -s spx-web -n spx-web -d
tmux send-keys -t spx-web 'cd ~/projects/spx-web' C-m
tmux send-keys -t spx-web 'vim' C-m
tmux send-keys -t spx-web '\n' C-m
tmux split-window -v -p 10 -t spx-web
tmux send-keys -t spx-web:1.1 'cd ~/projects/spx-web' C-m
tmux split-window -h -p 40 -t spx-web:1.1
tmux send-keys -t spx-web:1.2 'cd ~/projects/spx-web' C-m
tmux send-keys -t spx-web:1.2 'tail -f server.out' C-m




